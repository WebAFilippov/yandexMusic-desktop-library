{
  "name": "yandex-music-desktop-library",
  "version": "2.0.3",
  "description": "TypeScript library for controlling Yandex Music desktop application on Windows. Provides event-based and promise-based APIs for media control and volume management.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "win32"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "yandex",
    "music",
    "desktop",
    "windows",
    "media",
    "controller",
    "typescript"
  ],
  "author": "WebAFilippov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebAFilippov/yandexMusic-desktop-library.git"
  },
  "bugs": {
    "url": "https://github.com/WebAFilippov/yandexMusic-desktop-library/issues"
  },
  "homepage": "https://github.com/WebAFilippov/yandexMusic-desktop-library#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "bin/win-x64/libSkiaSharp.dll",
    "bin/win-x64/YandexMusicController.exe",
    "README.md",
    "LICENSE"
  ]
}